<p id="notice"><%= notice %></p>

<div class="container-fluid">
  <%= render "partials/verticleColumn" %>

  <!-- <div class="col-md-3">
    <div class="card">
      <div class="card-header">
        Manage Profile
      </div> -->

  <div class="offset-md-1 col-md-8">
    <div class="card">
      <div class="card-header">
        General Information
      </div>

    </div>
    <div class="borderVerticleColumn">
      <table>
          <tr>
            <th>First Name:</th>
            <td><%= @student.first_name %></td>
          </tr>
          <tr>
            <th>Last Name:</th>
            <td><%= @student.last_name %></td>
          </tr>
          <tr>
            <th>Email:</th>
            <td><%= @student.email %></td>
          </tr>
          <tr>
            <th>Password:</th>
            <td><%= @student.password_digest %></td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>


<p>
  <strong>Password:</strong>
  <%= @student.password_digest %>
</p>

<p>
  <strong>Linkedin:</strong>
  <%= @student.linkedin %>
</p>

<p>
  <strong>Github:</strong>
  <%= @student.github %>
</p>

<p>
  <strong>Website:</strong>
  <%= @student.website %>
</p>

<p>
  <strong>Contact:</strong>
  <%= @student.contact %>
</p>

<p>
  <strong>Description:</strong>
  <%= @student.description %>
</p>

<p>
  <strong>Course:</strong>
  <% @student.courses.each do |course| %>
  <td><%= course.name %></td>
  <%end%>
</p>

<p>
  <strong>Event:</strong>
  <% @student.event.name %>
</p>


<%= link_to 'Edit', edit_student_path(@student) %> |
<%= link_to 'Back', students_path %>
